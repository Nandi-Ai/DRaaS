[Unit]
Description=Consumer
After=network.target

[Service]
Type=simple
Restart=always
User=devops
WorkingDirectory= /home/devops/DRaaS
ExecStart=/home/devops/DRaaS/venv/bin/python /home/devops/DRaaS/consumer.py
StandardOutput=append:/var/log/consumer.log
StandardError=append:/var/log/consumer_error.log
SyslogIdentifier=consumer

[Install]
WantedBy=multi-user.target